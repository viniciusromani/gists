# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def ui_pods
	pod 'SnapKit', '~> 4.0.0'
end

def net_pods
	pod 'Alamofire'
	pod 'SwiftyJSON'
	pod 'Kingfisher', '~> 4.0'
end

target 'GithubGists' do
	# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
	use_frameworks!
	
	# Pods for GithubGists
	ui_pods
	net_pods
end
